    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "babel-preset-gatsby": "2.20.0",
    "autoprefixer": "10.4.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "28.1.3",
    "browserslist": "4.21.3",
    "concurrently": "7.3.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
