{
  "version": "3.0.0",
  "name": "gatsby-starter-lumen",
  "description": "A minimal, lightweight and mobile-first starter for creating blazing-fast static blogs",
  "main": "n/a",
  "scripts": {
    "develop": "yarn run clean && gatsby develop",
    "build": "yarn run clean && export NODE_OPTIONS=--max_old_space_size=4096 && gatsby build",
    "deploy": "yarn run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint:js": "eslint --cache --ext .js,.jsx .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\" \"yarn flow\"",
    "test": "jest --config ./jest/jest-config.js",
    "test:coverage": "jest --coverage --config ./jest/jest-config.js",
    "test:watch": "jest --watch --config ./jest/jest-config.js",
    "release": "standard-version"
  },
  "repository": "https://github.com/smith-30/gatsby-blog",
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "netlify-cms",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme",
    "starter",
    "disqus"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "classnames": "^2.3.1",
    "codecov": "^3.8.2",
    "gatsby": "^4.1.2",
    "gatsby-link": "^4.20.0",
    "gatsby-plugin-catch-links": "^4.20.0",
    "gatsby-plugin-feed": "^4.20.0",
    "gatsby-plugin-flow": "^3.20.0",
    "gatsby-plugin-google-gtag": "^4.20.0",
    "gatsby-plugin-manifest": "^4.20.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-netlify-cms": "^6.20.0",
    "gatsby-plugin-offline": "^5.20.0",
    "gatsby-plugin-react-helmet": "^5.20.0",
    "gatsby-plugin-sass": "^5.20.0",
    "gatsby-plugin-sharp": "^4.20.0",
    "gatsby-plugin-sitemap": "^5.20.0",
    "gatsby-remark-autolink-headers": "^5.1.1",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^5.20.0",
    "gatsby-remark-draw": "^1.0.16",
    "gatsby-remark-graphviz": "^4.20.0",
    "gatsby-remark-images": "^6.1.1",
    "gatsby-remark-katex": "^6.20.0",
